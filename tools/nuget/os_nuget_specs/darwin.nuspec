<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <% if (pkg.id)                       { %><id><%=                       pkg.id                       %></id><%                       } %>
    <% if (pkg.version)                  { %><version><%=                  pkg.version                  %></version><%                  } %>
    <% if (pkg.authors)                  { %><authors><%=                  pkg.authors                  %></authors><%                  } %>
    <% if (pkg.owners)                   { %><owners><%=                   pkg.owners                   %></owners><%                   } %>
    <% if (pkg.licenseUrl)               { %><licenseUrl><%=               pkg.licenseUrl               %></licenseUrl><%               } %>
    <% if (pkg.packageUrl)               { %><projectUrl><%=               pkg.packageUrl               %></projectUrl><%               } %>
    <% if (pkg.iconUrl)                  { %><iconUrl><%=                  pkg.iconUrl                  %></iconUrl><%                  } %>
    <% if (pkg.requireLicenseAcceptance) { %><requireLicenseAcceptance><%= pkg.requireLicenseAcceptance %></requireLicenseAcceptance><% } %>
    <% if (pkg.description)              { %><description><%=              pkg.description              %></description><%              } %>
    <% if (pkg.releaseNotes)             { %><releaseNotes><%=             pkg.releaseNotes             %></releaseNotes><%             } %>
    <% if (pkg.copyright)                { %><copyright><%=                pkg.copyright                %></copyright><%                } %>
    <% if (pkg.tags)                     { %><tags><%=                     pkg.tags                     %></tags><%                     } %>
    <dependencies>
    </dependencies>
  </metadata>
  <files>
    <file src="README.md" target="content\\README.md" />
  </files>
</package>